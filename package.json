{
  "name": "astro-paper",
  "version": "2.3.0",
  "private": false,
  "scripts": {
    "dev": "astro check --watch & astro dev",
    "start": "astro dev",
    "install": "deno install --allow-scripts=npm:sharp@0.33.5,npm:@swc/core@1.7.36",
    "build": "astro build --config astro.config-static.mjs && jampack ./dist/client",
    "deploy-build": "astro build",
    "preview": "astro preview",
    "sync": "astro sync",
    "astro": "astro",
    "format:check": "prettier --plugin-search-dir=. --check .",
    "format": "prettier --plugin-search-dir=. --write .",
    "cz": "cz",
    "lint": "eslint ."
  },
  "dependencies": {
    "@astrojs/check": "^0.9.3",
    "@astrojs/node": "^8.3.4",
    "@astrojs/rss": "^4.0.7",
    "@deno/astro-adapter": "^0.1.3",
    "astro": "^4.15.10",
    "astro-icon": "^1.1.0",
    "astro-navbar": "^2.3.2",
    "astro-seo": "^0.8.3",
    "fuse.js": "^7.0.0",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.3",
    "prettier-plugin-astro": "^0.13.0",
    "remark-collapse": "^0.1.2",
    "remark-toc": "^9.0.0",
    "satori": "^0.10.13",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@resvg/resvg-js": "^2.6.2",
    "@astrojs/react": "^3.6.2",
    "@astrojs/sitemap": "^3.1.6",
    "@astrojs/tailwind": "^5.1.1",
    "@divriots/jampack": "^0.24.0",
    "@tailwindcss/typography": "^0.5.13",
    "@types/github-slugger": "^1.3.0",
    "@types/react": "^18.3.1",
    "astro-eslint-parser": "^1.0.2",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.2.0",
    "eslint-plugin-astro": "^1.1.2",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,mdx,json}": [
      "prettier --plugin-search-dir=. --write"
    ]
  }
}
